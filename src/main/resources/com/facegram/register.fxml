<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="480.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.facegram.controllers.RegisterController">
   <children>
      <Label layoutX="109.0" layoutY="104.0" text="Register">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <TextField fx:id="tfName" layoutX="89.0" layoutY="194.0" promptText="Name" />
      <TextField fx:id="tfPassword" layoutX="89.0" layoutY="248.0" promptText="Password" />
      <Button fx:id="btnCancel" layoutX="37.0" layoutY="352.0" mnemonicParsing="false" text="Cancel" />
      <Button fx:id="btnRegister" layoutX="238.0" layoutY="352.0" mnemonicParsing="false" onAction="#register" text="Register" />
   </children>
</AnchorPane>
