   <children>
      <Button fx:id="buttonAdd" layoutX="195.0" layoutY="278.0" mnemonicParsing="false" onAction="#addButton" prefHeight="39.0" prefWidth="110.0" text="Public Comment" />
      <TextArea fx:id="textComment" layoutX="13.0" layoutY="14.0" prefHeight="251.0" prefWidth="294.0" promptText="Insert Here Your Comment" wrapText="true" />
      <Button fx:id="buttonClose" layoutX="14.0" layoutY="278.0" mnemonicParsing="false" onAction="#cancelButton" prefHeight="39.0" prefWidth="110.0" text="Back" />
   </children>
</AnchorPane>
